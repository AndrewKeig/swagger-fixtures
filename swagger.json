{
	"definitions": {
		"User": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
          "format": "int64",
          "example": "12345"
				},
				"username": {
          "type": "string",
          "example": "airasoul"
				},
				"firstName": {
          "type": "string",
          "example": "andrew"
				},
				"lastName": {
          "type": "string",
          "example": "keig"
				},
				"email": {
          "type": "string",
          "example": "andrew.keig@gmail.com"
				},
				"password": {
					"type": "string",
          "example": "password"
				},
        "phones" : {
          "type" : "array",
          "items" : {
            "type" : "object",
            "properties" : {
              "number" : {
                "description" : "List of phone numbers",
                "example" : "07943818181",
                "type" : "string"
              }
            }
          }
        },
        "dimensions" : {
          "type" : "object",
          "properties" : {
            "weight" : {
              "description" : "weight in kg",
              "example" : 180,
              "type" : "number"
            },
            "height" : {
              "description" : "height in metres",
              "example" : 120,
              "type" : "number"
            }
          }
        },
				"userStatus": {
					"type": "integer",
					"format": "int32",
          "description": "User Status",
          "example": "1"
				}
			}
		}
	}
}